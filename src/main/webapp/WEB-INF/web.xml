<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  
<!--   Servlets -->
  
  <servlet>
  	<servlet-name>ServletPrueba</servlet-name>
  	<display-name>ServletPrueba</display-name>
  	<description></description>
  	<servlet-class>servlet.ServletPrueba</servlet-class>
  </servlet>
  
    <servlet>
  	<servlet-name>ConexionJSP</servlet-name>
  	<display-name>ConexionJSP</display-name>
  	<description></description>
  	<servlet-class>servlet.ConexionJSPServlet</servlet-class>
  </servlet>

  <servlet>
  	<servlet-name>LoginServlet</servlet-name>
  	<display-name>LoginServlet</display-name>
  	<description></description>
  	<servlet-class>servlet.LoginServlet</servlet-class>
  </servlet>

<!-- USER SERVLETS -->
  <servlet>
  	<servlet-name>UserServlet</servlet-name>
  	<display-name>UserServlet</display-name>
  	<description></description>
  	<servlet-class>servlet.UserServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>UserListServlet</servlet-name>
  	<display-name>UserListServlet</display-name>
  	<description></description>
  	<servlet-class>servlet.UserListServlet</servlet-class>
  </servlet>

  
<!--   MAPPING -->
  
  <servlet-mapping>
  	<servlet-name>ServletPrueba</servlet-name>
  	<url-pattern>/ServletPrueba</url-pattern>
  </servlet-mapping>
   <servlet-mapping>
  	<servlet-name>ConexionJSP</servlet-name>
  	<url-pattern>/ConexionJSP</url-pattern>
  </servlet-mapping>
 
  <servlet-mapping>
  	<servlet-name>LoginServlet</servlet-name>
  	<url-pattern>/LoginServlet</url-pattern>
  </servlet-mapping>

	<!-- USER MAPPING -->
  <servlet-mapping>
  	<servlet-name>UserServlet</servlet-name>
  	<url-pattern>/UserServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>UserListServlet</servlet-name>
  	<url-pattern>/UserListServlet</url-pattern>
  </servlet-mapping>



</web-app>
